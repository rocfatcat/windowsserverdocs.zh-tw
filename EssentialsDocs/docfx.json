{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "disable_contribution_on_translation_quality": [
        "MT",
        "MTE75",
        "MTE95",
        "Blank"
      ],
      "_op_documentIdPathDepotMapping": {
        "./": {
          "depot_name": "WS.EssentialsDocs"
        }
      }
    },
      "fileMetadata": {
         "ms.prod": {
            "**/*.md": "windows-server"
         },
         "ms.technology": {
            "**/*.md": "essentials"
         }
      },
    "template": [],
    "dest": "window-server-docs/essentials",
    "markdownEngineName": "markdig"
  }
}
